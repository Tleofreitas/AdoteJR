    <ImageView
        android:id="@+id/imgBtnPesquisar"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_marginStart="76dp"
        android:layout_marginTop="64dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView3"
        app:srcCompat="@drawable/ic_pesquisar" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Pesquisar"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/imgBtnPesquisar"
        app:layout_constraintStart_toStartOf="@+id/imgBtnPesquisar"
        app:layout_constraintTop_toBottomOf="@+id/imgBtnPesquisar" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Cadastrar"
        android:textColor="@color/black"
        android:textSize="16sp"
        android:textStyle="normal"
        app:layout_constraintEnd_toEndOf="@+id/imgBtnCadastrar"
        app:layout_constraintStart_toStartOf="@+id/imgBtnCadastrar"
        app:layout_constraintTop_toBottomOf="@+id/imgBtnCadastrar" />

